@model IEnumerable<PhotoShare.Models.Photo>

@{
    ViewBag.Title = "Index";
}


<div class="latest-photos-list">
    <h2>Latest Photos (@Model.Count())</h2>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>

    <br />

    @foreach (var photo in Model)
    {
        @Html.Partial("_Photo", photo);
    }

</div>
